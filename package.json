{
  "name": "typenote",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.1",
  "scripts": {
    "postinstall": "electron-rebuild",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "test": "pnpm -r --filter '!@typenote/integration-tests' test",
    "test:integration": "pnpm --filter @typenote/integration-tests test",
    "test:all": "pnpm test && pnpm test:integration",
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "prepare": "husky",
    "mutate": "pnpm -r --filter './packages/*' mutate",
    "dev": "pnpm --filter @typenote/desktop dev",
    "start": "pnpm --filter @typenote/desktop dev"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.2",
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "concurrently": "^9.2.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
